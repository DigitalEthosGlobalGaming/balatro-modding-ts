{
  "name": "balatro-modding-ts",
  "version": "1.0.0",
  "description": "Balatro TS is a project that aims to build your mod in LUA and automatically convert it to whatever the mod loading is for Balatro.",
  "scripts": {
    "dev": "concurrently \"npm run dev-tools\" \"npm run dev-modding-framework\"",
    "setup": "concurrently \"npm run setup-tools\" \"npm run setup-modding-framework\"",
    "dev-tools": "cd tools && npm run dev",
    "dev-modding-framework": "cd modding-framework && npm run dev",
    "setup-tools": "cd tools && npm install",
    "setup-modding-framework": "cd modding-framework && npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
